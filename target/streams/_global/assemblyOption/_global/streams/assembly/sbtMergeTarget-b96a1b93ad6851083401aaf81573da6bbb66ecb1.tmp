

nueva_infra {
  include "ofertador-defaults.conf"
  include "nueva-infra.conf"
}

include "uat_sources_prod.conf"

include "uat_nueva_infra.conf"

include "uat.conf"

include "martin.conf"

include "agustin.conf"

akka.http.client.connecting-timeout=600s
# Esto esta para limitar la cantidad de claves leidas
bounded-mailbox {
  mailbox-type = "akka.dispatch.BoundedMailbox"
  mailbox-capacity = 50000
  mailbox-push-timeout-time = 24h
}

dispatchers {
  consumer {
    type = Dispatcher
    executor = "fork-join-executor"
    fork-join-executor {
      parallelism-min = 5
      parallelism-factor = 2.0
      parallelism-max = 10
    }
    throughput = 5
  }
}

local {

  include "zelig/zelig-dev.conf"

  include "zelig/newbizrulesdebug.conf"
}


userservice-context {
  type = Dispatcher
  executor = "thread-pool-executor"
  thread-pool-executor {
    fixed-pool-size = 32
  }
  throughput = 1
}

catalogservice-context {
  type = Dispatcher
  executor = "thread-pool-executor"
  thread-pool-executor {
    fixed-pool-size = 32
  }
  throughput = 1
}

batchrecommenderservice-context {
  type = Dispatcher
  executor = "thread-pool-executor"
  thread-pool-executor {
    fixed-pool-size = 32
  }
  throughput = 1
}



dispatchers.kafka {
  type = Dispatcher
  executor = "thread-pool-executor"
  thread-pool-executor {
    fixed-pool-size = 32
  }
  throughput = 1
}

dispatchers.akka-stream {
  type = Dispatcher
  executor = "thread-pool-executor"
  thread-pool-executor {
    fixed-pool-size = 32
  }
  throughput = 1
}

